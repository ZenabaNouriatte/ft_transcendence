{
  "uid": "bf2jrbxbv198gc", 
  "title": "FT â€“ Core Overview",
  "tags": ["ft-transcendence", "core"],
  "timezone": "browser",
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "version": 1,
  "time": { "from": "now-15m", "to": "now" },
  "panels": [
    {
      "id": 1,
      "title": "Containers Healthy",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [{ "refId": "A", "expr": "sum(up)" }],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "Active WS Connections",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        { "refId": "A", "expr": "sum(ws_connections{job=\"gateway\"}) or on() vector(0)" }
      ],
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 0 }
    },
    {
      "id": 3,
      "title": "Users Online (DB)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        { "refId": "A", "expr": "sum(users_online{job=\"gateway\"}) or on() vector(0)" }
      ],
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 6 }
    },
    {
      "id": 4,
      "title": "Chat (Last 5 min)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        { "refId": "A", "expr": "sum(chat_total_last_5m{job=\"gateway\"}) or on() vector(0)" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 6 }
    },
    {
      "id": 5,
      "title": "CPU Services - Vue d'ensemble (services actifs)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(process_cpu_seconds_total[1m]) * 100",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "orientation": "horizontal",
        "displayMode": "gradient"
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 12 }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [{ "type": "dashboard", "name": "Annotations & Alerts", "enable": true, "hide": true }] }
}

